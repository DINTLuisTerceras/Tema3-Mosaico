<Window x:Class="Tema3_Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema3_Mosaico"
        mc:Ignorable="d"
        Title="Mosaico" Height="650" Width="700" Icon="assets/Barkerbaggies-Pool-Ball-Cue.ico">
    <DockPanel>
        <TextBlock Background="DarkViolet" Width="100" DockPanel.Dock="Left"></TextBlock>
        <TextBlock Background="DarkViolet" Height="100" DockPanel.Dock="Top"></TextBlock>
        <TextBlock Background="Green" Height="100" DockPanel.Dock="Bottom"></TextBlock>
        <TextBlock Background="Green" Width="100" DockPanel.Dock="Right"></TextBlock>
        <TextBlock Background="OrangeRed" Height="100" DockPanel.Dock="Bottom"></TextBlock>
        <TextBlock Background="OrangeRed" Width="100" DockPanel.Dock="Left"></TextBlock>
        <TextBlock Background="DeepPink" Height="100" DockPanel.Dock="Top"></TextBlock>
        <TextBlock Background="DeepPink" Width="100" DockPanel.Dock="Right"></TextBlock>
        <TextBlock Background="Black"></TextBlock>

    </DockPanel>
</Window>
